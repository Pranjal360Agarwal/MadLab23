<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="anchorPane" prefHeight="100.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.iandw.musicplayerjavafx.ListViewController">
   <children>
      <FlowPane prefHeight="47.0" prefWidth="216.0">
         <children>
            <TextField fx:id="listViewTextInput" prefHeight="27.0" prefWidth="195.0" promptText="playlist title">
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
               <FlowPane.margin>
                  <Insets left="10.0" right="10.0" top="15.0" />
               </FlowPane.margin>
            </TextField>
         </children>
      </FlowPane>
      <Button fx:id="okButton" layoutX="37.0" layoutY="59.0" mnemonicParsing="false" onMouseClicked="#okButtonClicked" prefHeight="25.0" prefWidth="52.0" scaleShape="false" text="OK" />
      <Button fx:id="cancelButton" layoutX="119.0" layoutY="59.0" mnemonicParsing="false" onMouseClicked="#cancelButtonClicked" text="Cancel" />
   </children>
</AnchorPane>
